@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <script type="text/javascript" src="@Url.Content("~/Scripts/SlideShow.js")"></script>
    <title>Home</title>
</head>
<body>
    <div class="content-wrapper">
        <br />
        <div class="bar">
            <span class="barText">
                <h2>
                    Welcome!
                </h2>
            </span>
        </div>
        <br />
        <br />
        @if (ViewData.Model.Count > 0)
        {
            @Html.Partial("_HomeImageScroll")
        }
        @Html.Partial("_Page")
    </div>
</body>
</html>
